{
  "version": 1,
  "publicAddress": "localhost",
  "httpPort": 9001,
  "codechain": {
    "host": "http://localhost:7070",
    "networkId": "bm"
  },
  "sequelize": {
    "dialect": "postgres",
    "operatorsAliases": false,
    "host": "localhost",
    "pool": {
      "min": 1,
      "max": 10,
      "handleDisconnects": true
    },
    "retry": {
      "max": 5
    }
  },
  "worker": {
    "watchSchedule": "*/10 * * * * *"
  }
}
